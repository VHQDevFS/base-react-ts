{
  "name": "base-react-ts",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --mode development --port 3600 --host",
    "dev:https": "vite --mode development --port 3600 --host --https",
    "dev:local": "vite --mode dev --port 3600 --host",
    "build": "vite build --mode production",
    "preview": "vite preview",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,less,sass,scss,md}\"",
    "lint": "yarn format && eslint --max-warnings=0 --fix \"./src/**/*.{js,jsx,ts,tsx,json}\"",
    "ts-check": "yarn tsc --noEmit",
    "prepare": "husky install",
    "gen:theme-typings": "chakra-cli tokens ./src/themes/index.ts",
    "postinstall": "yarn run gen:theme-typings",
    "test": "vitest",
    "test-coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@chakra-ui/cli": "latest",
    "@chakra-ui/react": "^2.4.9",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@hookform/resolvers": "^2.9.10",
    "@lukemorales/query-key-factory": "^1.1.0",
    "@nikolovlazar/chakra-ui-prose": "^1.2.1",
    "@tanstack/react-query": "^4.24.4",
    "@tanstack/react-query-devtools": "^4.24.4",
    "axios": "^1.3.0",
    "chakra-react-select": "^4.4.3",
    "dayjs": "^1.11.7",
    "dompurify": "^2.4.3",
    "framer-motion": "^8.5.5",
    "html-react-parser": "^3.0.8",
    "logt": "^1.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.43.0",
    "react-hot-toast": "^2.4.0",
    "react-icons": "^4.7.1",
    "react-router-dom": "^6.7.0",
    "react-textarea-autosize": "^8.4.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "@vitejs/plugin-legacy": "^3.0.2",
    "@vitejs/plugin-react-swc": "^3.0.1",
    "@vitest/coverage-c8": "^0.28.3",
    "enzyme": "^3.11.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-import-order": "^2.1.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "terser": "^5.16.2",
    "typescript": "^4.9.5",
    "vite": "^4.0.4",
    "vite-plugin-checker": "^0.5.5",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-remove-console": "^1.3.0",
    "vitest": "^0.28.3"
  }
}
